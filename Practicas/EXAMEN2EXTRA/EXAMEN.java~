import java.util.*;
public class EXAMEN
{
    public static void main(String[]args){
    Scanner leer=new Scanner(System.in);
    System.out.println("Introduce la frase");
    String f=leer.nextLine();
    String es="";
    boolean p=false;
    for(int i=0;i<f.length();i++){
        if(f.charAt(i)!=' '){
        es+=f.charAt(i);
        }
    }
    f=es;
    int x=f.length()-1;
    int nl=0;
    for(int i=0;i<f.length();i++){
        if(f.charAt(i)==f.charAt(x)){
        nl++;
        }
        x--;
    }
    if(nl==f.length()){
    p=true;    
    System.out.print(p);    
    }
    else{
    System.out.print(p);
    }
    }
}
